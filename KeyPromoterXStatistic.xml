<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Value">
        <Item>
          <IdeaActionID>XDebugger.CopyValue</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy Value</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>90</Count>
        </Item>
      </Statistic>
      <Statistic Action="Evaluate Expression...">
        <Item>
          <IdeaActionID>EvaluateExpression</IdeaActionID>
          <ShortCut>'Alt+F8'</ShortCut>
          <Description>Evaluate Expression...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'widgets.kts'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'widgets.kts'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop">
        <Item>
          <IdeaActionID>RunDashboard.Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>17</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>